<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service
            id="ivory.http_adapter.data_collector"
            class="Ivory\HttpAdapterBundle\DataCollector\IvoryHttpAdapterDataCollector"
        >
            <tag name="data_collector" template="IvoryHttpAdapterBundle:DataCollector:layout" id="ivory.http_adapter" />
            <tag name="ivory.http_adapter.subscriber" />
        </service>
    </services>
</container>
